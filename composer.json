{
    "name": "rinzler/user-encryption",
    "description": "User asymmetric encryption wrapper using private and public keys for Laravel.",
    "authors": [
        {
            "name": "Alex Jarmoszuk",
            "email": "alex@rinzler.ch"
        }
    ],
    "minimum-stability": "dev",
    "require": {
        "php": ">=7.1.3",
        "laravel/laravel": ">=5.5",
        "paragonie/halite": "^4.4"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Rinzler\\UserEncryption\\UserEncryptionServiceProvider"
            ],
            "aliases": {
                "UserEncryption": "Rinzler\\UserEncryption\\UserEncryption"
            }
        }
    },
    "autoload": {
        "psr-4": {
            "Rinzler\\UserEncryption\\": "src/"
        }
    }
}
